<view class='page__bd'>
  <text class='titel2'>{{M_Name}}</text>
  <text class='titel1'>检测时间： </text>
  <text class='titel'> {{time}}</text>
  <form report-submit bindsubmit='orderSign'>
    <view>
      <text class='titel'>{{time}}</text>
    </view>
    <view>
      <text class='name'>检测项目</text>
      <text class='name1'>检测值</text>

      <text class='first'>蛋白质</text>
      <text class='yes'>{{listData2}} g/L</text>
    </view>
    <view>
      <text class='first1'>钙离子</text>
      <text class='yes1'>{{listData1}} mg/L</text>
    </view>
    <view>
      <text class='first2'>锌离子</text>
      <text class='yes2'>{{listData3}} mg/L</text>
    </view>
    <view>
    <text class='S_Pro'>您的蛋白质含量打败了全国<text class='bold'>{{S_Pro}}</text>%</text>
    <text class='S_Cal'>您的钙含量打败了全国<text class='bold'>{{S_Cal}}</text>%</text>
    <text class='S_Zn'>您的锌含量打败了全国<text class='bold'>{{S_Zn}}</text>%</text>
    </view>
    <text class='result1'>您的{{result1}}</text>
    <text class='result2'>您的{{result2}}</text>
    <text class='result3'>您的{{result3}}</text>
  </form>
</view>
<view class='img-box'>
<image  class='image' src="../../img/112.png" ></image>
</view>